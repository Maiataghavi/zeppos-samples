{
  "configVersion": "v3",
  "app": {
    "appId": 20001,
    "appName": "Notification",
    "appType": "app",
    "version": {
      "code": 1,
      "name": "1.0.0"
    },
    "icon": "icon.png",
    "vender": "zepp",
    "description": "A sample app for notifications"
  },
  "permissions": [
    "data:os.device.info",
    "device:os.notification",
    "device:os.alarm"
  ],
  "runtime": {
    "apiVersion": {
      "compatible": "3.0",
      "target": "3.0",
      "minVersion": "3.0"
    }
  },
  "targets": {
    "gt": {
      "module": {
        "page": {
          "pages": ["page/index"]
        },
        "app-service": {
          "services": ["app-service/index", "app-service/delay"]
        }
      },
      "platforms": [
        {
          "st": "r",
          "dw": 480
        }
      ],
      "designWidth": 480
    }
  },
  "i18n": {
    "en-US": {
      "appName": "Notification"
    }
  },
  "defaultLanguage": "en-US"
}
